{
  "manifest_version": 2,
  "name": "Interactive Summary",
  "version": "0.5.0",
  "description": "Display an interactive summary for webpage. Navigate easier thanks to this extension",

  "permissions": [
    "activeTab",
    "<all_urls>"
  ],
  "sidebar_action": {
    "browser_style": true,
    "default_title": "Interactive Summary",
    "default_panel": "index.html",
    "default_icon": "icon/icon"
  },
  "commands": {
    "_execute_sidebar_action": {
      "suggested_key": {
        "default": "Alt+O"
      }
    }
  }
}
